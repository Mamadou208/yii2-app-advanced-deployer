{
    "name": "jacmoe/yii2-app-advanced-deployer",
    "description": "Yii 2 Advanced Project Template with Deployer.php support",
    "keywords": ["yii2", "framework", "advanced", "deployer", "project template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/jacmoe/yii2-app-advanced-deployer/issues?state=open",
        "source": "https://github.com/jacmoe/yii2-app-advanced-deployer"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
